{
    "name":"github.com/gschat/ioschat",

    "domain":"gslang",

    "import" : [
        {"name" : "github.com/gsrpc/gsrpc","domain":"task|gslang","version":"release/v1.0"},
        {"name" : "github.com/gschat/gschat","domain":"gslang","version":"develop"}
    ],


    "properties":{
        "gsrpc":{
            "main":["github.com/gsrpc/gsrpc","github.com/gschat/gschat/gschat.gs","github.com/gschat/gschat/shared.gs"]
        },

        "objrpc":{

            "main":{
                "output":"./gsrpc",
                "skips":["gsrpc/[^/]*\\.gs"]
            }
        }
    }
}
